{
  "settings": {
    "number_of_shards": 3,
    "number_of_replicas": 1,
    "index": {
      "max_result_window": 10000
    }
  },
  "mappings": {
    "properties": {
      "uniqueId": {
        "type": "keyword"
      },
      "sectorCode": {
        "type": "keyword"
      },
      "sectorName": {
        "type": "text",
        "analyzer": "ik_max_word",
        "search_analyzer": "ik_smart",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 64
          }
        }
      },
      "sectorType": {
        "type": "keyword"
      },
      "tradeDate": {
        "type": "date",
        "format": "yyyy-MM-dd||epoch_millis"
      },
      "changePercent": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "mainNetInflow": {
        "type": "double"
      },
      "mainNetInflowRatio": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "superLargeInflow": {
        "type": "double"
      },
      "superLargeInflowRatio": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "largeInflow": {
        "type": "double"
      },
      "largeInflowRatio": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "mediumInflow": {
        "type": "double"
      },
      "mediumInflowRatio": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "smallInflow": {
        "type": "double"
      },
      "smallInflowRatio": {
        "type": "scaled_float",
        "scaling_factor": 100
      },
      "leadStock": {
        "type": "keyword"
      },
      "leadStockName": {
        "type": "text",
        "analyzer": "ik_max_word",
        "search_analyzer": "ik_smart",
        "fields": {
          "keyword": {
            "type": "keyword",
            "ignore_above": 64
          }
        }
      },
      "crawlTime": {
        "type": "date",
        "format": "yyyy-MM-dd HH:mm:ss||yyyy-MM-dd||epoch_millis"
      }
    }
  }
}